console.log(2>1)
console.log(2>=1)
console.log(2<=1)
console.log(2==1)
console.log(2!=1)


//avoid these
console.log("2">1)
console.log("02">1)
//avoid these
console.log(null>0)
console.log(null==0)
console.log(null>=0) 
//avoid these
console.log(undefined==0)
console.log(undefined>0)
console.log(undefined>=0) 


//===
//strict check
//check value and data types
console.log("2"===2)//not allowed in triple equal




